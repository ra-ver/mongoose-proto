  "version": "0.2.0",
  "configurations": [{
    "type": "node",
    "request": "launch",
    "cwd": "${workspaceFolder}",
    "name": "Launch Program",
    "program": "${workspaceFolder}/buildScripts/executeApp.js",
    "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/babel-node",
    "runtimeArgs": ["--presets", "env", "--nolazy", "--inspect=5858"],
    "stopOnEntry": false,
    "args": [],
    "preLaunchTask": null,
    "env": {
      "NODE_ENV": "DEV"
    },
    "console": "integratedTerminal",
    "sourceMaps": false,
    "outFiles": [],
    "port": 5858
  }]
}
